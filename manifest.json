{
    "manifest_version": 2,
    "name": "My Budget Connector Creator",
    "version": "0.0.1",
    "browser_action": {
        "default_icon": "test.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "downloads"
    ]
}